package QualityKiosksTraining.CalculatorAPI;

public class Calculator 
{

	public int Addition(int num1,int num2)
	{
		int Res=num1+num2;
		return Res;
		///return 0;
	}
	
	public int Subtraction(int num1,int num2)
	{
		
		int Res=num1-num2;
		return Res;
	}

	public int Multiplication(int num1,int num2)
	{
		
		int Res=num1*num2;
		return Res;
	}
}
